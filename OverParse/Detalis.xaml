<Window x:Class="OverParse.Detalis"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OverParse"
        mc:Ignorable="d"
        WindowStartupLocation="CenterOwner"
        Title="Detalis" Height="300" Width="300">
    <Grid>
        <Label Content="ID" x:Name="Data1" HorizontalAlignment="Left" VerticalAlignment="Top" Padding="0"/>
        <Label Content="Name" x:Name="Data2" HorizontalAlignment="Left" VerticalAlignment="Top" Padding="0" Margin="0,21,0,0"/>
        <Label Content="Damage" x:Name="Data3" HorizontalAlignment="Left" VerticalAlignment="Top" Padding="0" Margin="0,42,0,0"/>
        <Label Content="MaxHit" x:Name="Data4" HorizontalAlignment="Left" VerticalAlignment="Top" Padding="0" Margin="0,63,0,0" />
    </Grid>
</Window>
